<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TechDispoMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:TechDispoMobile.Pages">


    <FlyoutItem Title="Accueil">
        <ShellContent Title="Accueil" ContentTemplate="{DataTemplate local:MainPage}" Route="main"/>
    </FlyoutItem> 
    <FlyoutItem Title="Weather">
        <ShellContent Title="Météo" ContentTemplate="{DataTemplate pages:WeatherPage}" Route="weather"/>
    </FlyoutItem>

</Shell>
    
    